#pragma kernel ClearVisible
#pragma kernel StampVisible
#pragma kernel AccumulateExplored
// Textures
RWTexture2D<float> _Visible;
RWTexture2D<float> _Explored;

// Units: float3(xWorld, zWorld, level)
