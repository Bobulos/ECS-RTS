int _UnitCount;
int2 _FogDim;
float3 _WorldMin;
float3 _WorldMax;

// ----------------- CLEAR VISIBLE -----------------
[numthreads(8, 8, 1)]
void ClearVisible(uint3 id : SV_DispatchThreadID)
{
    if (id.x >= _FogDim.x || id.y >= _FogDim.y)
        return;
    _Visible[id.xy] = 0;
}

